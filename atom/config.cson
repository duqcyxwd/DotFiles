"*":
  "atom-beautify":
    bash:
      beautify_on_save: true
      indent_size: 4
    js:
      beautify_on_save: false
      default_beautifier: "ESLint Fixer"
      end_with_newline: true
    json:
      beautify_on_save: false
      default_beautifier: "Prettier"
      end_with_newline: true
    jsx:
      disabled: true
    markdown:
      disabled: true
  "atom-ctags":
    buildTimeout: 30000
  "atom-focus-mode":
    focusModeLineOpacity: "100%"
    focusShadowMode:
      numberOfLinesToHighlightAboveCursor: 10
      numberOfLinesToHighlightBelowCursor: 10
    whenFocusModeIsActivated:
      enterFullScreen: false
      hideLineWrapGuide: true
      useTypeWriterMode: true
  "atom-fuzzy-grep":
    minSymbolsToStartSearch: 1
    showFullPath: true
  "atom-runner": {}
  "autocomplete-plus":
    suppressActivationForEditorClasses: [
      "narrow"
    ]
  "command-palette":
    historyCommands: true
    maxHistoryCommands: 3
    preserveLastSearch: true
  core:
    audioBeep: false
    closeDeletedFileTabs: true
    disabledPackages: [
      "linter-clojure"
      "intentions"
      "sourcefetch"
      "sidekit-welcome"
      "goto-definition"
      "sidekit"
      "linter-documents"
      "git-plus"
      "goto"
      "symbol-gen"
      "atom-ctags"
      "tester"
      "document-outline"
      "markdown-table-formatter"
      "linter-jshint"
      "language-babel"
      "Hydrogen"
      "TabNine"
      "timecop"
      "test-tt"
      "parinfer"
      "pigments"
      "tester-mocha"
      "ask-stack"
      "atom-beautify"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*.zip"
      "*.tar"
      ".tag*"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "git-projects"
      "git-plus"
      "move-panes"
      "swap-panes"
      "diff-pane"
      "zentabs"
      "open-on-bitbucket"
      "atom-fuzzy-grep"
      "atom-focus-mode"
      "emmet"
    ]
    telemetryConsent: "no"
    themes: [
      "nord-atom-ui"
      "one-dark-vivid-syntax"
    ]
    versionPinnedPackages: [
      "ink"
      "proto-repl"
    ]
  editor:
    fontSize: 16
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
  "emmet-jsx-css-modules": {}
  "exception-reporting":
    userId: "1b4b7798-4072-4ce0-8f66-402e684121da"
  "format-shell":
    indent: 4
  "fuzzy-finder":
    ignoredNames: [
      ".git"
      ".*"
      "node_modules*"
      "target*"
    ]
    preserveLastSearch: true
    searchAllPanes: true
  "git-projects":
    rootPath: "/Users/chuan.du/repo;/Users/chuan.du/github;/Users/chuan.du/os-repo;"
    sortBy: "Latest modification date"
  github:
    gitDiagnostics: true
    performanceToConsole: true
  linter: {}
  "linter-eslint": {}
  "linter-jshint":
    jshintFileName: "/Users/chuan.du/repo/sidekit/resources/.jshintrc"
  "linter-ui-default":
    panelHeight: 180
    panelRepresents: "Entire Project"
    showPanel: true
    statusBarClickBehavior: "Jump to next issue"
  "markdown-preview":
    liveUpdate: false
  minimap: {}
  "my-package":
    thingVolume: "10"
  "one-dark-ui":
    fontSize: 16
    stickyHeaders: true
  "open-on-bitbucket": {}
  pigments: {}
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
    prettierEslintOptions:
      prettierLast: true
    useEslint: true
    useStylelint: true
  "project-manager":
    sortBy: "last modified"
  "proto-repl":
    leinPath: "/Users/chuan.du/bin/lein"
  sidekit:
    customizationApiUrl: "http://med25.cenx.localnet:8080/customization/"
    lintScope: "Entire Project"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  tester:
    testStateStyle: "Icon"
  "tester-mocha": {}
  "todo-show":
    findTheseTodos: [
      "TODO"
      "FIXME"
      "HACK"
      "NOTE"
      "COMBAK"
      "TEMP"
      "REMOVETAG"
    ]
    ignoreThesePaths: [
      "node_modules"
      "vendor"
      "bower_components"
      ".tags"
      "scratch*"
      "*.md"
      "*hint.js"
      "esprima*"
      "jvm-npm.js"
      "assets/*"
    ]
    showInTable: [
      "File"
      "Text"
      "Type"
    ]
    sortBy: "Type"
  "toggle-packages":
    togglePackages: [
      "atom-beautify"
      "prettier-atom"
      "pigments"
      "tester-mocha"
    ]
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  "vim-mode-plus":
    automaticallyEscapeInsertModeOnActivePaneItemChange: true
    blackholeRegisteredOperators: [
      "change*"
      "substitute*"
    ]
    clearHighlightSearchOnResetNormalMode: true
    highlightSearchExcludeScopes: [
      "narrow"
    ]
    keymapSemicolonToInnerAnyPairInVisualMode: true
    showHoverSearchCounter: true
    smoothScrollOnFullScrollMotion: true
    smoothScrollOnHalfScrollMotion: true
    smoothScrollOnHalfScrollMotionDuration: 600
    useClipboardAsDefaultRegister: true
  "vim-mode-plus-ex-mode":
    notifiedUseExMode: true
  welcome:
    showOnStartup: false
  "welcome-side-kit": {}
  whitespace:
    keepMarkdownLineBreakWhitespace: true
  zentabs:
    neverCloseDirty: true
    neverCloseNew: true
    neverCloseUnsaved: true
