{
  "happy": {
    "prefix": "happy",
    "body": [
      "f = io.open(${1:\"${2:filename}\"}, \"${3:r}\")\n",
      "while true do",
      "\tline = f:read()",
      "\tif line == nil then break end\n",
      "\t${0:-- code}",
      "end"
    ],
    "description": "read file line by line"
  },
  "nvim_print": {
    "prefix": "np",
    "body": [
      "nvim_print(${1})"
    ],
    "description": "Nice print"
  }
}
